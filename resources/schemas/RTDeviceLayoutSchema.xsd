<xs:schema xmlns="http://www.aadi.no/RTDeviceLayoutSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.aadi.no/RTDeviceLayoutSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Layout">
		<xs:annotation>
			<xs:documentation>Represents the root element in the Device Layout schema.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="HardwareLayout">
					<xs:annotation>
						<xs:documentation>Hardware layout.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Time" type="xs:dateTime" use="required">
				<xs:annotation>
					<xs:documentation>Layout timestamp.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ID" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Layout ID.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Version" type="xs:string" use="optional" default="1"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="HardwareLayout">
		<xs:annotation>
			<xs:documentation>Represents the hardware layout.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Instrument" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Instrument/platform settings.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ComPorts" minOccurs="0">
					<xs:annotation>
						<xs:documentation>List of COM ports.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="ComPort" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>List of COM ports.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="Serial" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Serial sensor layout.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Digital" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Digital sensor layout.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Analog" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Analog sensor layout.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="SR10VR22" minOccurs="0">
					<xs:annotation>
						<xs:documentation>SR10/VR22 sensor layout (on Datalogger 3860).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Communication" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Communication equipment, for example satelitte modems.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Serial">
		<xs:annotation>
			<xs:documentation>Represents the layout for all serial sensors.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CustomFormats" minOccurs="0">
					<xs:annotation>
						<xs:documentation>List of custom data formats.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="CustomFormat" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>List of custom data formats.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PredefinedFormats" minOccurs="0">
					<xs:annotation>
						<xs:documentation>List of pre-defined data formats.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="PredefinedFormat" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>List of pre-defined data formats.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Sensors">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Sensor" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Sensor">
											<xs:sequence>
												<xs:element name="ComPort" type="xs:string">
													<xs:annotation>
														<xs:documentation>COM port number.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ComPortModeID" type="xs:int">
													<xs:annotation>
														<xs:documentation>COM port mode ID.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="FormatTypeID" type="xs:int">
													<xs:annotation>
														<xs:documentation>Data format type ID (pre-defined = 1, custom = 2).</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="FormatID" type="xs:string">
													<xs:annotation>
														<xs:documentation>Data format ID.</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="FormatVersionCode" type="xs:string">
													<xs:annotation>
														<xs:documentation>Data format version code.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Channels">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Channel" type="Channel" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="SensorTypes">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="SensorType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="SerialDataFormat">
		<xs:annotation>
			<xs:documentation>Represents the base class for all serial sensor data formats.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VersionCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Data format version code. Updated each time the format definition is modified.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Descr" type="xs:string">
				<xs:annotation>
					<xs:documentation>Data format description.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Data format ID.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Sensor">
		<xs:annotation>
			<xs:documentation>Represents a single sensor.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ProductNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation>Product number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Product name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SerialNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation>Serial number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NodeTypeID" type="xs:int">
				<xs:annotation>
					<xs:documentation>AADI node type (category) ID.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SensorTypeID" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Sensor type ID.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Sensor ID.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Channel">
		<xs:annotation>
			<xs:documentation>Represents a single channel.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SensorID" type="xs:string">
				<xs:annotation>
					<xs:documentation>Sensor ID that this channel belongs to.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParamName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Parameter name.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Unit" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The physical unit of the parameter value, e.g. kPa or DegC.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RawValue" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Value indicating if this parameter contains a raw value (rather than a value in engineering units).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RangeMin" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Minimum parameter value.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RangeMax" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Maximum parameter value.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The data type of the parameter value.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Format" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number format of the parameter value.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChannelTypeID" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Channel type ID.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ChannelNumber" type="xs:int" use="required">
			<xs:annotation>
				<xs:documentation>Channel number.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="ComPort">
		<xs:annotation>
			<xs:documentation>Represents a serial com port.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Info" type="xs:string">
					<xs:annotation>
						<xs:documentation>Port information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Modes">
					<xs:annotation>
						<xs:documentation>List of available port modes.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Mode" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>List of available port modes.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Descr" type="xs:string">
											<xs:annotation>
												<xs:documentation>Port mode description, e.g. RS232.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ID" type="xs:int" use="required">
										<xs:annotation>
											<xs:documentation>Port mode ID.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PortNumber" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Port number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Instrument">
		<xs:annotation>
			<xs:documentation>Represents instrument/platform settings.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ProductNumber" type="xs:string">
					<xs:annotation>
						<xs:documentation>Instrument product number.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SerialNumber" type="xs:string">
					<xs:annotation>
						<xs:documentation>Instrument serial number.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ProductName" type="xs:string">
					<xs:annotation>
						<xs:documentation>Instrument product name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Parts" minOccurs="0">
					<xs:annotation>
						<xs:documentation>List of instrument parts.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Part" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>List of instrument parts.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="LANSettings" minOccurs="0">
					<xs:annotation>
						<xs:documentation>LAN chip settings.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="SensorGroupDefinition">
		<xs:annotation>
			<xs:documentation>Represents a group of sensors.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sensors">
				<xs:annotation>
					<xs:documentation>List of sensors in this group.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Sensor" type="Sensor" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>List of sensors in this group.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Channels">
				<xs:annotation>
					<xs:documentation>List of channels in this group.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Channel" type="Channel" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>List of channels in this group.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="SensorTypes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of sensor types.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="SensorType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>List of sensor types.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ChannelTypes" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of channel types.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ChannelType" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>List of channel types.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AvailableChannels">
				<xs:annotation>
					<xs:documentation>List of available channels.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ChannelNumber" type="xs:int" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>List of available channels.</xs:documentation>
							</xs:annotation>
